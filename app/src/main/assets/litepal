<litepal>
    <dbname value = "cool_weather" />   <!--将数据库名指定成cool_weather-->
    <version value = "1" />
    <list>
        <mapping class="com.example.db.Province"/>   <!--名字得是你java下的项目名-->
        <mapping class="com.example.db.City"/>   <!--将3个实体类添加到映射列表中-->
        <mapping class="com.example.db.County"/>
    </list>
</litepal>